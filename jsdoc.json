{
  "tags": {
    "allowUnknownTags": ["component"]
  },
  "plugins": ["node_modules/better-docs/component"],
  "source": {
    "include": ["src"],
    "includePattern": ".js$",
    "exludePattern": "(node_modules | docs)"
  },
  "templates": {
    "cleverLinks": false,
    "monospaceLinks": false
  },
  "opts": {
    "recurse": "true",
    "destination": "./docs",
    "template": "node_modules/better-docs"
  }
}